<h1>Stories</h1>
<div class="row">
  {{#each stories}}
  <div class="col s12 m4">
    <div class="card">
      <div class="card-content">
        <h5>{{title}}</h5>
        <p class="story-text">{{{body}}}</p>
        <br>
          {{user.firstName}}
      </div>
    </div>
  </div>
  {{else}}
  <p>No stories found</p>
  {{/each}}
</div>